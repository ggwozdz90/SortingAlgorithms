{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-generator",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Generator/Generator.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-sorter",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Sorter/Sorter.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-sorter-release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Sorter/Sorter.csproj",
                "--configuration",
                "Release"
            ],
            "problemMatcher": "$msCompile"
        },
    ]
}